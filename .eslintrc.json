{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "operator-linebreak": "off",
    "camelcase": "off",
    "no-unused-vars": "warn",
    "arrow-parens": "off",
    "class-methods-use-this": "off",
    "implicit-arrow-linebreak": "off",
    "import/no-cycle": "off"
  }
}
